            "dependsOn": [
                "[resourceId('Microsoft.Sql/servers/databases', parameters('servers_sogeti_sql_brandreputation_d_name'), 'sogeti-sqldb1-brandreputation-d')]",
                "[resourceId('Microsoft.Sql/servers', parameters('servers_sogeti_sql_brandreputation_d_name'))]"
