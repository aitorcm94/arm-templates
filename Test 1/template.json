{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_sogeti_vm_tcc_brandreputation_d_name": {
            "defaultValue": "sogeti-vm-tcc-brandreputation-d",
            "type": "String"
        },
        "disks_sogeti_datadisk_tcc_OsDisk_1_externalid": {
            "defaultValue": "/subscriptions/3f26cf74-4de5-43d8-9c14-945c5594b176/resourceGroups/SOGETI-APP-BRANDREPUTATION-D/providers/Microsoft.Compute/disks/sogeti-datadisk-tcc-OsDisk-1",
            "type": "String"
        },
        "networkInterfaces_sogeti_nic_tcc_brandreputation_d_externalid": {
            "defaultValue": "/subscriptions/3f26cf74-4de5-43d8-9c14-945c5594b176/resourceGroups/sogeti-app-brandreputation-d/providers/Microsoft.Network/networkInterfaces/sogeti-nic-tcc-brandreputation-d",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2021-11-01",
            "name": "[parameters('virtualMachines_sogeti_vm_tcc_brandreputation_d_name')]",
            "location": "westeurope",
            "tags": {
                "displayName": "VM",
                "projectName": "Brand Reputation Prototype",
                "billingIdentifier": "SogetiLab",
                "environmentType": "DEV"
            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_B2s"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2019-datacenter-smalldisk-g2",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "sogeti-datadisk-tcc-OsDisk-1",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "StandardSSD_LRS",
                            "id": "[parameters('disks_sogeti_datadisk_tcc_OsDisk_1_externalid')]"
                        },
                        "deleteOption": "Delete",
                        "diskSizeGB": 30
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "sogeti-vm-tcc-d",
                    "adminUsername": "adminUserTCC",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true,
                        "patchSettings": {
                            "patchMode": "AutomaticByOS",
                            "assessmentMode": "ImageDefault",
                            "enableHotpatching": false
                        }
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_sogeti_nic_tcc_brandreputation_d_externalid')]",
                            "properties": {
                                "deleteOption": "Delete"
                            }
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true
                    }
                }
            }
        }
    ]
}